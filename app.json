{
  "expo": {
    "name": "KnitCount",
    "slug": "KnitCount2",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/KnitCountIcon1024x1024.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FEC34C"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.romzalabs.knitcount",
      "buildNumber": "1.0.5",
      "supportsTablet": false,
      "infoPlist": {
        "NSCameraUsageDescription": "KnitCount uses the camera to add images to your projects."
      }
    },
    "android": {
      "package": "com.romzalabs.knitcount2",
      "versionCode": 2,
      "permissions": [
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "description": "KnitCount is the perfect way to store the stitch count for each of your knitting projects. Simple, intuitive design and animation makes for the cutest and easiest-to-use row counter available.",
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "romza-labs",
            "project": "knitcount-2",
            "authToken": "<REDACTED>"
          }
        }
      ]
    }
  }
}
